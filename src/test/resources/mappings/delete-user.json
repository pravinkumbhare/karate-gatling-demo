{
  "request": {
    "method": "DELETE",
    "urlPattern": "/user/[0-9]+"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": ["response-template"],
    "body": "{ \"status\": \"User deleted successfully\", \"userId\": {{request.path.[1]}} }"
  }
}
