{
  "request": {
    "method": "POST",
    "url": "/user"
  },
  "response": {
    "status": 201,
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": ["response-template"],
    "body": "{ \"status\": \"User created successfully\", \"userId\": {{randomInt 100 999}}, \"name\": \"{{jsonPath request.body '$.name'}}\", \"role\": \"{{jsonPath request.body '$.role'}}\" }"
  }
}
